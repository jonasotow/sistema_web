
.nodata
	color red
	font-weight bold
	text-transform uppercase
	text-align center
	font-size 16px

		
section 
	margin-top 1em
	padding-bottom 40px
	width 100%
	font-size 12px
	position absolute
	padding-bottom 0
	.panel 
		margin 0px auto 
		width 80%
	.panel-primary 
		width 90%
		border 0
		.panel-heading
			background-color #fff
			border 0
			text-transform uppercase
			color rgba(0,0,0,.87)
		.panel-body
			margin-top 0px
			margin-bottom 0px
			margin 0px auto
			width 100%
			form
				.buttonaction
					margin-top 20px
			.col-md-12
				.form-group
					form 
						display block
						.filtroflujo
							width 50%
							margin 0 auto
							select
								margin-bottom 10px
							.buttonaction
								margin-top 10px
		.panel-footer 
			border-top 1px solid #FFF
			padding 0px 

.row1
	.form-group
		h4
			text-transform uppercase
			text-align center
		form
			.col-md-6
				padding-left 0	
			label
				padding-right 5px
				padding-top 10px
				display block
			input
				width 50%
			.buttonaction
				margin-top	15px	
.row2
	background #fafafa
	border-radius 5px
	float right
	.box-group
		width 100%
		h4
			text-transform uppercase
			text-align center			
		.crop
			position relative
			overflow auto
			max-height 380px
			padding-left 0
			list-style none
			margin-bottom 0
			li
				color #757575 !important
				border-bottom 1px solid rgba(0,0,0,.1);
				padding 10px 15px
				border-radius 4px
				margin-bottom 5px
				article
					font-size 1em
					.data
						width 100%			
						display inline-block
						text-transform uppercase
						font-size 1em
						color #333
						span
							font-weight bold
					.button 
						display inline-block
					.button-block
						padding-top 3px
						display block
						text-align center
					.modal
						.modal-dialog
							margin 150px auto
							.modal-content
								.modal-body
									span
										font-weight bold
										text-transform uppercase
								.success
									background #5cb85c
									color #fff
								.danger
									background #c9302c
									color #fff
								.vim 
									background-color #428bca
									button
									h4
										color #fff
			.nodata
				color red
				font-weight bold
				text-align center
				font-size 1.4em
						
