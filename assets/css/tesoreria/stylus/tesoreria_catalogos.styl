		.panel-body
			margin-top 0px
			margin-bottom 0px
			margin 0px auto
			width 100%
			.catalogo
				padding-top 3em
				width 80%
				margin 0 auto
				text-align center
				a 
					margin-bottom 1em
		.panel-footer 
			border-top 1px solid #FFF
			padding 0px
			
.homecatalogos
	margin-left 15%
	margin-top 10%
	padding-bottom 40px
	width 85%
	font-size 12px
	position absolute
	padding-bottom 0
	text-align center	
	text-transform uppercase
	.panel 
		margin 0px auto 
		width 70%
		background #eee
		padding-top 1em
		padding-bottom 1em
		h1
			font-size 4em
		.alerts
			text-align center
			color red	
			font-size 1.5em
												
.money
	position fixed
	top 120px
	left 230px
	text-transform uppercase
	.cash 
		width 180px
		padding-bottom 20px
		
	.tab-content
		padding-top 16px
	
		.tab-pane
			.table
				thead
					tr 
						th 
							text-align center		
																		
@media screen and (max-width: 990px)
	.row1
		margin-right 0
		margin-bottom 2em	
	.row2
		border-left 0
																	
								
@media screen and (max-width: 700px)
	.list-group
		a
			span 
				display none
		.aside-text 			
		.list-group-item 
		.sublist
			a
				span
					display none							
					